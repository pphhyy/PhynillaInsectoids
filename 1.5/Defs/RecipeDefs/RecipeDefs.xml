<?xml version="1.0" encoding="UTF-8"?>

<Defs>

    <RecipeDef Name= "GestatingRecipeBase" Abstract= "true">
        <workSpeedStat>DrugCookingSpeed</workSpeedStat>
        <workSkill>Cooking</workSkill>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Brewing</soundWorking>
        <allowMixingIngredients>true</allowMixingIngredients>
        <workAmount>1000</workAmount>
        <targetCountAdjustment>1</targetCountAdjustment>
        <recipeUsers>
		   <li>pphhyy_GestationWorkbench</li>
		</recipeUsers>
    </RecipeDef>

    <!-- Scarabs -->

<!-- Mega Scarab -->

    <RecipeDef ParentName= "GestatingRecipeBase">
        <defName>pphhyy_GestatingEggMegaScarab</defName>
        <label>make gestating mega scarab egg </label>
        <description>PLACEHOLDER</description>
        <jobString>making a gestating mega scarab egg</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Insectoid_Egg</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>pphhyy_InsectJelly</li>
                    </thingDefs>
                </filter>
                <count>25</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Insectoid_Egg</li>
                <li>pphhyy_InsectJelly</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <pphhyy_GestatingEggMegaScarab>1</pphhyy_GestatingEggMegaScarab>
        </products>
    </RecipeDef>

