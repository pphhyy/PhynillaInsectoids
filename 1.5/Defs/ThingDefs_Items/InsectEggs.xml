<Defs>

    <!-- Bases -->

    <ThingDef Name= "pphhyy_GestatingEggsBase" ParentName="ResourceBase" Abstract= "true">
        <healthAffectsPrice>false</healthAffectsPrice>
        <rotatable>false</rotatable>
        <stackLimit>10</stackLimit>
        <thingCategories>
            <li>Manufactured</li>
        </thingCategories>
        <statBases>
            <DeteriorationRate>1.0</DeteriorationRate>
            <MarketValue>2.5</MarketValue>
            <Mass>0.8</Mass>
            <Flammability>0.2</Flammability>
            <MaxHitPoints>60</MaxHitPoints>
        </statBases>
        <tickerType>Rare</tickerType>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>90</daysToRotStart>
                <rotDestroys>true</rotDestroys>
                <disableIfHatcher>true</disableIfHatcher>
            </li>
        </comps>
    </ThingDef>

    <ThingDef Name= "ChrysalisBase" ParentName="EggFertBase" Abstract= "true">
